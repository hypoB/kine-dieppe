<!DOCTYPE html>
<html lang="fr">
    <head>
        <title>{% block title %}Centre Kiné Sport Dieppe{% endblock %}</title>

        <meta charset="utf-8">
        <meta content="width=device-width, initial-scale=1.0" name="viewport">

        <meta name="description"
              content="Le centre kiné sport Dieppe propose un ensemble de soins thérapeutiques conventionnés dans un cadre privilégié pour la kinésithérapie et la masso-kinésithérapie (physiotherapy).">

        <link rel="canonical" href="https://cks-dieppe.fr/"/>

        {# Favicons #}
        <link href="assets/cks-dieppe.ico" rel="icon">
        <link href="assets/img/apple-touch-icon.png" rel="apple-touch-icon">

        {# Google Fonts #}
        <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

        <!-- Open Graph / Facebook -->
        <meta property="og:type" content="website">
        <meta property="og:url" content="https://cks-dieppe.fr/">
        <meta property="og:title" content="Centre Kiné Sport Dieppe - Accueil">
        <meta property="og:description" content="Le cabinet propose un ensemble de soins thérapeutiques conventionnés dans un cadre privilégié pour la masso-kinésithérapie (physiotherapy).">
        <meta property="og:image" content="">

        <!-- Twitter -->
        <meta property="twitter:card" content="summary_large_image">
        <meta property="twitter:url" content="https://cks-dieppe.fr/">
        <meta property="twitter:title" content="Centre Kiné Sport Dieppe - Accueil">
        <meta property="twitter:description" content="Le cabinet propose un ensemble de soins thérapeutiques conventionnés dans un cadre privilégié pour la masso-kinésithérapie (physiotherapy).">
        <meta property="twitter:image" content="">


        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
            <link href="assets/vendor/fontawesome-free/css/all.min.css" rel="stylesheet">
            <link href="assets/vendor/animate.css/animate.min.css" rel="stylesheet">
            <link href="assets/vendor/aos/aos.css" rel="stylesheet">
            <link href="assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
            <link href="assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
            <link href="assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
            <link href="assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
            <link href="assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">
            <link href="assets/css/style.css" rel="stylesheet">
        {% endblock %}
    </head>
    <body>
        {{ include('layouts/partials/_top_bar.html.twig')}}
        {{ include('layouts/partials/_nav.html.twig')}}

        <main id="main">
        {% block body %}

        {% endblock %}
        </main>

        {{ include('layouts/partials/_footer.html.twig')}}
        {{ include('layouts/partials/_back_to_top.html.twig')}}


        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}

            <script src="assets/vendor/purecounter/purecounter.js"></script>
            <script src="assets/vendor/aos/aos.js"></script>
            <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
            <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
            <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
{#            <script src="assets/vendor/php-email-form/validate.js"></script>#}

            <script src="assets/js/main.js"></script>
        {% endblock %}
    </body>
</html>
